{
  "manifest_version": 2,
  "name": "One Table",
  "version": "0.1",
  "description": "One Table",
  "icons":
  {
    "16": "icon/icon@16.png",
    "48": "icon/icon@48.png",
    "128": "icon/icon@128.png"
  },
  "background":
  {
    "page": "index.html"
  },
  "browser_action":
  {
    "default_icon": "icon/icon.png",
    "default_title": "One Table",
    "default_popup": "index.html"
  },
  "permissions":
  [
    "tabs",
    "storage"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}